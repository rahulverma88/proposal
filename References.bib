	keywords = {Civil Engineering, film flow, fluid distribution, Fluids, Geotechnical Engineering, Hydrogeology, Industrial Chemistry/Chemical Engineering, Mechanics, pore-scale displacement mechanisms, spreading coefficient, Thermodynamics, Three-phase flow},
	pages = {105--133},
	file = {Full Text PDF:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/R4GRM665/Øren and Pinczewski - 1995 - Fluid distribution and pore-scale displacement mec.pdf:application/pdf;Full Text PDF:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/V2VKESKE/Øren and Pinczewski - 1995 - Fluid distribution and pore-scale displacement mec.pdf:application/pdf;Snapshot:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/A3ER3FRH/BF00616927.html:text/html;Snapshot:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/C3PBB9SX/BF00616927.html:text/html;Snapshot:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/DCZ2WGQ5/BF00616927.html:text/html}
