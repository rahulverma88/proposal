	volume = {49},
	copyright = {©2013. American Geophysical Union. All Rights Reserved.},
	issn = {1944-7973},
	url = {http://onlinelibrary.wiley.com.ezproxy.lib.utexas.edu/doi/10.1002/wrcr.20334/abstract},
	doi = {10.1002/wrcr.20334},
	abstract = {We present a level set method for simulating capillary-controlled displacements with nonzero contact angles in porous media. The main feature of the method is a level set evolution velocity which is different in the pore space and solid phase. This augments the standard level set equation with an extra term such that, at steady state, the contact angle is enforced in the solid phase, whereas capillary and interfacial forces are balanced in the pore space. We employ the method to simulate quasistatic drainage and imbibition processes for different contact angles in several pore geometries, and to compute capillary pressure and fluid/fluid specific interfacial area curves in each case. We validate the method by comparing stable fluid configurations computed in idealized two-dimensional geometries and three-dimensional (3-D) straight tubes with known analytical solutions. Simulations performed in a subset of a 3-D sandstone image show that the developed method accounts for well-known pore-scale mechanisms such as piston-like invasion, Haines jump, interface coalescence, and retraction, swelling of wetting films and snap-off. The contact angle is formed by an intersection of the fluid/fluid interface and the void/solid boundary. Therefore, the solid matrix surrounding the pore space is discretized with at least an equal number of grid points as the size of the numerical stencil used to approximate the level set derivatives.},
	language = {en},
	number = {8},
	urldate = {2013-09-27},
	journal = {Water Resources Research},
	author = {Jettestuen, Espen and Helland, Johan O. and Prodanović, Maša},
	year = {2013},
	keywords = {3-D pore geometry, 3D pore geometry, capillary pressure, Contact angle, Level set method},
	pages = {4645--4661},
	file = {Full Text PDF:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/TBB2TBX9/Jettestuen et al. - 2013 - A level set method for simulating capillary-contro.pdf:application/pdf;Full Text PDF:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/XIXIQ72Z/Jettestuen et al. - 2013 - A level set method for simulating capillary-contro.pdf:application/pdf;Snapshot:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/VJXMT6FW/abstract.html:text/html;Snapshot:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/QRQ93I2U/abstract.html:text/html}
}

@article{harting_large-scale_2005,
