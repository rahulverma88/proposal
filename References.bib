	keywords = {adaptive mesh refinement, level set, octree data structure, particles, physics-based animation, smoke, water},
	pages = {457--462},
	file = {p457-losasso.pdf:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/9BH656PA/p457-losasso.pdf:application/pdf}
}

@article{hilden_numerical_2003,
