@article{ryazanov_two-phase_2009-1,
	title = {Two-{Phase} {Pore}-{Network} {Modelling}: {Existence} of {Oil} {Layers} {During} {Water} {Invasion}},
	volume = {80},
	issn = {0169-3913, 1573-1634},
	shorttitle = {Two-{Phase} {Pore}-{Network} {Modelling}},
	url = {http://link.springer.com/article/10.1007/s11242-009-9345-x},
	doi = {10.1007/s11242-009-9345-x},
	abstract = {Pore-network modelling is commonly used to predict capillary pressure and relative permeability functions for multi-phase flow simulations. These functions strongly depend on the presence of fluid films and layers in pore corners. Recently, van Dijke and Sorbie (J. Coll. Int. Sci. 293:455–463, 2006) obtained the new thermodynamically derived criterion for oil layers existence in the pore corners with non-uniform wettability caused by ageing. This criterion is consistent with the thermodynamically derived capillary entry pressures for other water invasion displacements and it is more restrictive than the previously used geometrical layer collapse criterion. The thermodynamic criterion has been included in a newly developed two-phase flow pore network model, as well as two versions of the geometrical criterion. The network model takes as input networks extracted from pore space reconstruction methods or CT images. Furthermore, a new n-cornered star shape characterization technique has been implemented, based on shape factor and dimensionless hydraulic radius as input parameters. For two unstructured networks, derived from a Berea sandstone sample, oil residuals have been estimated for different wettability scenarios, by varying the contact angles in oil-filled pores after ageing from weakly to strongly oil-wet. Simulation of primary drainage, ageing and water invasion show that the thermodynamical oil layer existence criterion gives more realistic oil residual saturations compared to the geometrical criteria. Additionally, a sensitivity analysis has been carried out of oil residuals with respect to end-point capillary pressures. For strongly oil-wet cases residuals increase strongly with increasing end-point capillary pressures, contrary to intermediate oil-wet cases.},
	language = {en},
	number = {1},
	urldate = {2015-12-07},
	journal = {Transport in Porous Media},
	author = {Ryazanov, A. V. and Dijke, M. I. J. van and Sorbie, K. S.},
	month = feb,
	year = {2009},
	keywords = {Civil Engineering, Classical Continuum Physics, Geotechnical Engineering, Hydrogeology, Industrial Chemistry/Chemical Engineering, Network modelling, Non-uniform wettability, Oil layer, Residual oil, Two-phase flow},
	pages = {79--99},
	file = {Full Text PDF:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/NUVQ77ZP/Ryazanov et al. - 2009 - Two-Phase Pore-Network Modelling Existence of Oil.pdf:application/pdf;Snapshot:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/RDAXIMAI/s11242-009-9345-x.html:text/html}
}

@inproceedings{losasso_simulating_2004,
	address = {New York, NY, USA},
