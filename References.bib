	keywords = {Civil Engineering, film flow, Fluids, Geotechnical Engineering, Hydrogeology, Industrial Chemistry/Chemical Engineering, Mechanics, micromodel, Multiphase flow, NAPL, non-spreading oil., spreading coefficient, Thermodynamics, Three-phase flow},
	pages = {277--297},
