}

@article{raeini_direct_2014,
	title = {Direct simulations of two-phase flow on micro-{CT} images of porous media and upscaling of pore-scale forces},
	volume = {74},
	issn = {0309-1708},
	url = {http://www.sciencedirect.com/science/article/pii/S0309170814001730},
	doi = {10.1016/j.advwatres.2014.08.012},
	abstract = {Pore-scale forces have a significant effect on the macroscopic behaviour of multiphase flow through porous media. This paper studies the effect of these forces using a new volume-of-fluid based finite volume method developed for simulating two-phase flow directly on micro-CT images of porous media. An analytical analysis of the relationship between the pore-scale forces and the Darcy-scale pressure drops is presented. We use this analysis to propose unambiguous definitions of Darcy-scale viscous pressure drops as the rate of energy dissipation per unit flow rate of each phase, and then use them to obtain the relative permeability curves. We show that this definition is consistent with conventional laboratory/field measurements by comparing our predictions with experimental relative permeability. We present single and two-phase flow simulations for primary oil injection followed by water injection on a sandpack and a Berea sandstone. The two-phase flow simulations are presented at different capillary numbers which cover the transition from capillary fingering at low capillary numbers to a more viscous fingering displacement pattern at higher capillary numbers, and the effect of capillary number on the relative permeability curves is investigated. Overall, this paper presents a new finite volume-based methodology for the detailed analysis of two-phase flow directly on micro-CT images of porous media and upscaling of the results to the Darcy scale.},
	urldate = {2016-01-02},
	journal = {Advances in Water Resources},
	author = {Raeini, Ali Q. and Blunt, Martin J. and Bijeljic, Branko},
	month = dec,
	year = {2014},
	keywords = {Direct simulation, Micro-CT images, Pore-scale modelling, porous media, Two-phase flow, Upscaling},
	pages = {116--126},
	file = {Raeini_et_al_AWR_2014.pdf:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/NQN86WQ2/Raeini_et_al_AWR_2014.pdf:application/pdf}
}

@article{blunt_relative_1991,
	title = {Relative permeabilities from two- and three-dimensional pore-scale network modelling},
	volume = {6},
