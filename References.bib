@article{vogel_morphological_1997,
	title = {Morphological determination of pore connectivity as a function of pore size using serial sections},
	volume = {48},
	issn = {1365-2389},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-2389.1997.tb00203.x/abstract},
	doi = {10.1111/j.1365-2389.1997.tb00203.x},
	abstract = {The geometry of pore space in soil is considered to be the key in understanding transport of water, gas and solute. However, a quantitative and explicit characterization, by means of a physical interpretation, is difficult because of the geometric complexity of soil structure.Pores larger than 40 μm within two soil horizons have been analysed morphologically on 3-dimensional digital representations of the pore space obtained by serial sections through impregnated specimens. The Euler-Poincaré characteristic has been determined as an index of connectivity in three dimensions. The pore connectivity is quantified as a function of the minimum pore diameter considered leading to a connectivity function of the pore space. Different pore size classes were distinguished using 3-dimensional erosion and dilation. The connectivity function turned out to differentiate between two soil materials. The pore space in an upper Ah horizon is intensely connected through pores between 40 and 100 μm, in contrast to the pore space in the AhBv beneath it. The morphological pore-size distributions were compared to the pore-size distribution obtained by water retention measurements. The discrepancy between these different methods corresponds to the expectation due to pore connectivity.},
	language = {en},
	number = {3},
	urldate = {2013-09-04},
	journal = {European Journal of Soil Science},
	author = {Vogel, H. J.},
	year = {1997},
	pages = {365--377},
	file = {Full Text PDF:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/KJTR5FT9/Vogel - 1997 - Morphological determination of pore connectivity a.pdf:application/pdf;Snapshot:/Users/rahul/Library/Application Support/Zotero/Profiles/j37u5a3p.default/zotero/storage/ZRVFT2JR/abstract.html:text/html}
}

