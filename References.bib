@article{mitropoulos_kelvin_2008,
	title = {The {Kelvin} equation},
	volume = {317},
	issn = {0021-9797},
	url = {http://www.sciencedirect.com/science/article/pii/S0021979707014403},
	doi = {10.1016/j.jcis.2007.10.001},
	abstract = {The capillary condensation/evaporation process is studied in conjunction with small angle X-ray scattering measurements. The scattering data are analyzed with the indirect Fourier transformation technique and the results are compared with the predictions of the Kelvin equation. It is found that the Kelvin equation is obeyed by menisci with mean radius of curvature as low as 40 Å. For smaller radii, in particular from 40 to 30 Å, the two methods differ by ∼25\%. Broekhoff and de Boer analysis may improve the prediction. The hysteresis region of an adsorption step is presented schematically.},
