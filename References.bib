	keywords = {Civil Engineering, Fluids, Geotechnical Engineering, Hydrogeology, Industrial Chemistry/Chemical Engineering, Mechanics, Multiphase flow, Pore-scale modeling, relative permeability, Thermodynamics},
	pages = {23--41},
